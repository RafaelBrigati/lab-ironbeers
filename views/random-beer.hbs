<h1>Random Beer Page</h1>

<div class='contBeers'>
  {{#each randomBeer}}       
    {{> partials}}          
    <h3>Food Pairing:</h3>
    <ul>
      {{#each food_pairing}} 
        <li>{{this}}</li>
      {{/each}}
    </ul>
    <h3>Brewers Tips:</h3>
    <p>{{brewers_tips}}</p>
  {{/each}}
</div>